# Convex deployment URL (needed for scripts/test-query.ts)
CONVEX_URL=https://your-deployment.convex.cloud

# OpenAI API key for embeddings
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-...

# Pinecone API key for vector search
# Get from: https://app.pinecone.io/ -> API Keys
PINECONE_API_KEY=...

# Anthropic API key for Claude
# Get from: https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=sk-ant-...

# Firecrawl API key for web scraping (needed for data ingestion)
# Get from: https://firecrawl.dev
FIRECRAWL_API_KEY=...

# Geocodio API key for address resolution (optional)
# Get from: https://dash.geocod.io/ -> API Keys
# Free tier: 2500 lookups/day
GEOCODIO_API_KEY=...
